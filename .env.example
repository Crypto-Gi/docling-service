# Docling Service example environment configuration

# Core limits
DOCLING_MAX_UPLOAD_MB=25
DOCLING_STORAGE_SOFT_LIMIT_MB=200
DOCLING_STORAGE_HARD_LIMIT_MB=250

# Cloud storage (set to true to enable R2 uploads)
DOCLING_CLOUD_STORAGE_ENABLED=false
DOCLING_CLOUD_STORAGE_PROVIDER=local
DOCLING_CLOUD_UPLOAD_MODE=sync
DOCLING_CLOUD_KEEP_LOCAL=false

# Cloudflare R2 credentials (required when provider=cloudflare_r2)
DOCLING_R2_ACCOUNT_ID=
DOCLING_R2_ACCESS_KEY_ID=
DOCLING_R2_SECRET_ACCESS_KEY=
DOCLING_R2_BUCKET_NAME=
DOCLING_R2_REGION=auto
DOCLING_R2_PUBLIC_URL_BASE=
